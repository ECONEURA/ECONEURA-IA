{
  "app_settings_template": {
    "version": "1.0.0",
    "last_updated": "2025-09-10T00:30:00Z",
    "environment": "dev"
  },
  "api_app_settings": {
    "NODE_ENV": "production",
    "PORT": "8080",
    "HOST": "0.0.0.0",
    "LOG_LEVEL": "info",
    "CORS_ORIGIN": "https://econeura-devweb.azurewebsites.net",
    "RATE_LIMIT_WINDOW_MS": "900000",
    "RATE_LIMIT_MAX_REQUESTS": "100",
    "SESSION_SECRET": "{{SESSION_SECRET}}",
    "JWT_SECRET": "{{JWT_SECRET}}",
    "JWT_EXPIRES_IN": "24h",
    "REFRESH_TOKEN_EXPIRES_IN": "7d",
    "DATABASE_URL": "postgresql://{{POSTGRES_USER}}:{{POSTGRES_PASSWORD}}@{{POSTGRES_HOST}}:5432/{{POSTGRES_DATABASE}}?sslmode=require",
    "REDIS_URL": "redis://{{REDIS_HOST}}:6380?password={{REDIS_PASSWORD}}&ssl=true",
    "APPLICATIONINSIGHTS_CONNECTION_STRING": "{{APPLICATIONINSIGHTS_CONNECTION_STRING}}",
    "AZURE_OPENAI_ENDPOINT": "{{AZURE_OPENAI_ENDPOINT}}",
    "AZURE_OPENAI_API_KEY": "{{AZURE_OPENAI_API_KEY}}",
    "AZURE_OPENAI_DEPLOYMENT_NAME": "{{AZURE_OPENAI_DEPLOYMENT_NAME}}",
    "MISTRAL_API_KEY": "{{MISTRAL_API_KEY}}",
    "MISTRAL_BASE_URL": "https://api.mistral.ai",
    "MAKE_WEBHOOK_URL": "{{MAKE_WEBHOOK_URL}}",
    "MAKE_WEBHOOK_HMAC_SECRET": "{{MAKE_WEBHOOK_HMAC_SECRET}}",
    "MAKE_ALLOWED_IPS": "{{MAKE_ALLOWED_IPS}}",
    "AZURE_TENANT_ID": "{{AZURE_TENANT_ID}}",
    "AZURE_CLIENT_ID": "{{AZURE_CLIENT_ID}}",
    "AZURE_CLIENT_SECRET": "{{AZURE_CLIENT_SECRET}}",
    "KEY_VAULT_URL": "https://{{KEY_VAULT_NAME}}.vault.azure.net/",
    "ENCRYPTION_KEY": "{{ENCRYPTION_KEY}}",
    "WEBHOOK_SECRET": "{{WEBHOOK_SECRET}}",
    "SENTRY_DSN": "{{SENTRY_DSN}}",
    "HEALTH_CHECK_INTERVAL": "30000",
    "MAX_REQUEST_SIZE": "10mb",
    "TRUST_PROXY": "true",
    "HELMET_CSP": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'",
    "HELMET_HSTS": "max-age=31536000; includeSubDomains",
    "CORS_CREDENTIALS": "true",
    "CORS_METHODS": "GET,POST,PUT,DELETE,OPTIONS",
    "CORS_HEADERS": "Content-Type,Authorization,X-Requested-With",
    "SWAGGER_ENABLED": "false",
    "SWAGGER_PATH": "/api-docs",
    "METRICS_ENABLED": "true",
    "METRICS_PATH": "/metrics"
  },
  "web_app_settings": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "https://econeura-devapi.azurewebsites.net",
    "NEXT_PUBLIC_APP_NAME": "ECONEURA",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0",
    "NEXT_PUBLIC_ENVIRONMENT": "dev",
    "NEXT_PUBLIC_SENTRY_DSN": "{{SENTRY_DSN}}",
    "NEXT_PUBLIC_ANALYTICS_ID": "{{ANALYTICS_ID}}",
    "NEXT_PUBLIC_FEATURE_FLAGS": "{{FEATURE_FLAGS}}",
    "NEXTAUTH_URL": "https://econeura-devweb.azurewebsites.net",
    "NEXTAUTH_SECRET": "{{NEXTAUTH_SECRET}}",
    "NEXTAUTH_PROVIDERS": "azure-ad",
    "AZURE_AD_CLIENT_ID": "{{AZURE_CLIENT_ID}}",
    "AZURE_AD_CLIENT_SECRET": "{{AZURE_CLIENT_SECRET}}",
    "AZURE_AD_TENANT_ID": "{{AZURE_TENANT_ID}}",
    "SESSION_SECRET": "{{SESSION_SECRET}}",
    "REDIS_URL": "redis://{{REDIS_HOST}}:6380?password={{REDIS_PASSWORD}}&ssl=true",
    "APPLICATIONINSIGHTS_CONNECTION_STRING": "{{APPLICATIONINSIGHTS_CONNECTION_STRING}}",
    "HEALTH_CHECK_INTERVAL": "30000",
    "MAX_REQUEST_SIZE": "10mb",
    "TRUST_PROXY": "true",
    "CORS_ORIGIN": "https://econeura-devapi.azurewebsites.net",
    "CORS_CREDENTIALS": "true",
    "CORS_METHODS": "GET,POST,PUT,DELETE,OPTIONS",
    "CORS_HEADERS": "Content-Type,Authorization,X-Requested-With",
    "SWAGGER_ENABLED": "false",
    "SWAGGER_PATH": "/api-docs",
    "METRICS_ENABLED": "true",
    "METRICS_PATH": "/metrics"
  },
  "secrets_required": [
    "SESSION_SECRET",
    "JWT_SECRET", 
    "POSTGRES_PASSWORD",
    "REDIS_PASSWORD",
    "APPLICATIONINSIGHTS_CONNECTION_STRING",
    "AZURE_OPENAI_API_KEY",
    "MISTRAL_API_KEY",
    "MAKE_WEBHOOK_HMAC_SECRET",
    "AZURE_CLIENT_SECRET",
    "ENCRYPTION_KEY",
    "WEBHOOK_SECRET",
    "SENTRY_DSN",
    "NEXTAUTH_SECRET",
    "ANALYTICS_ID"
  ],
  "key_vault_references": {
    "SESSION_SECRET": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/session-secret/)",
    "JWT_SECRET": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/jwt-secret/)",
    "POSTGRES_PASSWORD": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/postgres-password/)",
    "REDIS_PASSWORD": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/redis-password/)",
    "APPLICATIONINSIGHTS_CONNECTION_STRING": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/app-insights-connection-string/)",
    "AZURE_OPENAI_API_KEY": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/azure-openai-api-key/)",
    "MISTRAL_API_KEY": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/mistral-api-key/)",
    "MAKE_WEBHOOK_HMAC_SECRET": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/make-webhook-hmac-secret/)",
    "AZURE_CLIENT_SECRET": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/azure-client-secret/)",
    "ENCRYPTION_KEY": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/encryption-key/)",
    "WEBHOOK_SECRET": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/webhook-secret/)",
    "SENTRY_DSN": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/sentry-dsn/)",
    "NEXTAUTH_SECRET": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/nextauth-secret/)",
    "ANALYTICS_ID": "@Microsoft.KeyVault(SecretUri=https://{{KEY_VAULT_NAME}}.vault.azure.net/secrets/analytics-id/)"
  }
}