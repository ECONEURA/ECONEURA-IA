{
  "name": "@econeura/sdk",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc --watch",
    "generate": "npm run generate:types && npm run generate:client",
    "generate:types": "npx openapi-typescript ../../apps/api/openapi.json -o src/types.ts",
    "generate:client": "node scripts/generate-client.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:unit": "vitest --run --reporter verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
  "@econeura/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "openapi-typescript": "^7.9.1",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  }
}