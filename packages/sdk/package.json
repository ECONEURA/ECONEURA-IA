{
  "name": "@econeura/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for ECONEURA API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "openapi-typescript ../../apps/api/openapi/openapi.yaml -o src/types.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "openapi-typescript": "^7.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}