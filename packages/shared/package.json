{
  "name": "@econeura/shared",
  "version": "0.1.0",
  "description": "SDK tipado y utilidades compartidas para EcoNeura",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "econeura",
    "sdk",
    "api",
    "typescript"
  ],
  "author": "EcoNeura",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
