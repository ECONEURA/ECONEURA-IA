#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/dependency-cruiser@17.0.1/node_modules/dependency-cruiser/bin/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/dependency-cruiser@17.0.1/node_modules/dependency-cruiser/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/dependency-cruiser@17.0.1/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/dependency-cruiser@17.0.1/node_modules/dependency-cruiser/bin/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/dependency-cruiser@17.0.1/node_modules/dependency-cruiser/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/dependency-cruiser@17.0.1/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dependency-cruiser/bin/depcruise-baseline.mjs" "$@"
else
  exec node  "$basedir/../dependency-cruiser/bin/depcruise-baseline.mjs" "$@"
fi
