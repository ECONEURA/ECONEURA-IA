#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules/@redocly/cli/bin/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules/@redocly/cli/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules/@redocly/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules/@redocly/cli/bin/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules/@redocly/cli/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules/@redocly/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@redocly+cli@1.34.5_ajv@8.17.1/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@redocly/cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../@redocly/cli/bin/cli.js" "$@"
fi
