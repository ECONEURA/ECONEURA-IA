#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/madge@8.0.0_typescript@5.9.2/node_modules/madge/bin/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/madge@8.0.0_typescript@5.9.2/node_modules/madge/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/madge@8.0.0_typescript@5.9.2/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/madge@8.0.0_typescript@5.9.2/node_modules/madge/bin/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/madge@8.0.0_typescript@5.9.2/node_modules/madge/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/madge@8.0.0_typescript@5.9.2/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../madge/bin/cli.js" "$@"
else
  exec node  "$basedir/../madge/bin/cli.js" "$@"
fi
