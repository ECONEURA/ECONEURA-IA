#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.23.4_@types+node@24.5.2/node_modules/@openapitools/openapi-generator-cli/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.23.4_@types+node@24.5.2/node_modules/@openapitools/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.23.4_@types+node@24.5.2/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.23.4_@types+node@24.5.2/node_modules/@openapitools/openapi-generator-cli/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.23.4_@types+node@24.5.2/node_modules/@openapitools/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.23.4_@types+node@24.5.2/node_modules:/mnt/c/Users/Usuario/ECONEURA-IA-1.worktrees/fix/ci-generate-score-fix/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@openapitools/openapi-generator-cli/main.js" "$@"
else
  exec node  "$basedir/../@openapitools/openapi-generator-cli/main.js" "$@"
fi
